             xmlns:models="clr-namespace:MetroDemo.Models"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"   MouseDown="Grid_MouseDown">
    <Grid   Margin="20,0,20,10">
        <Grid.RowDefinitions>
            <RowDefinition  Height="920*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition  Width="920*" />
            <ColumnDefinition  Width="940*" />
        </Grid.ColumnDefinitions>

        <!--left-->
        <Grid   Margin="0,0,20,0" Grid.Column="0">
