                            <Border  Background="#A9CDEF"  CornerRadius="5"  Opacity="0.12"  Height="40" Width="132"  HorizontalAlignment="Left"/>
                        <TextBlock   x:Name="txtFan1"   Foreground="white" Margin="16 0 0 0" VerticalAlignment="Center"   Width="132" FontFamily="PingFangSC"  FontSize="18" >
                               运行
                            </TextBlock>
                        </Grid>
                    </StackPanel>

                <StackPanel Grid.Row="2" Grid.Column="0"  HorizontalAlignment="left" Orientation="Horizontal"  Margin="20 0 0 0"  >
                    <Image  Source="/Resources/charge/充电仓概览/充电中.png"  Stretch="Fill"  Width="56" Height="47" >
                    </Image>
                    <Grid>
                        <Border   CornerRadius="5"    Height="40" Width="172"  HorizontalAlignment="Left">
                            <Border.Background>
                                <LinearGradientBrush StartPoint="0,0" EndPoint="1,0" Opacity="0.1">
                                    <GradientStop Color="#A9CDEF" Offset="0.0" />
                                    <GradientStop Color="#722493D3" Offset="0.45" />
                                    <GradientStop Color="#334DBAFF" Offset="1" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                        <StackPanel Grid.Row="0"  Orientation="Horizontal"    Margin="0 0 20 0"  >
                            <TextBlock  Foreground="#FFFFFF" Margin="16 0 0 0" VerticalAlignment="Center"  FontFamily="Fontquan-XinYiGuanHeiTi"  FontWeight="Bold" FontSize="20" >
                              充电中：&#160;&#160;&#160;&#160;&#160;&#160;&#160;    
                            </TextBlock>
                            <TextBlock x:Name="txtChargingCount" Foreground="#00FDFA" Margin="16 0 0 0" VerticalAlignment="Center"  HorizontalAlignment="Right"  FontFamily="Fontquan-XinYiGuanHeiTi"  FontWeight="Bold" FontSize="20" >
                                  2
                            </TextBlock>
                        </StackPanel>
                    </Grid>
                </StackPanel>
                <StackPanel Grid.Row="2" Grid.Column="1"   Orientation="Horizontal"  Margin="20 0 0 0"  >
                    <TextBlock  Foreground="#CEDCE6" Margin="10 0 0 0" VerticalAlignment="Center"  FontFamily="PingFangSC"  FontSize="16" >
                       &#160;&#160;&#160;&#160;&#160;&#160;&#160;电池仓温度2：
                    </TextBlock>
                    <Grid>
                        <Border  Background="#A9CDEF"  CornerRadius="5"  Opacity="0.12"  Height="40" Width="132"  HorizontalAlignment="Left"/>
                        <TextBlock  x:Name="txttempLocation2"   Foreground="white" Margin="16 0 0 0" VerticalAlignment="Center"  FontFamily="PingFangSC"  FontSize="18" >
                            30℃
                        </TextBlock>
                    </Grid>
                </StackPanel>
                <StackPanel Grid.Row="2" Grid.Column="2"  HorizontalAlignment="left" Orientation="Horizontal"  Margin="20 0 20 0"  >
                    <TextBlock  Foreground="#CEDCE6" Margin="10 0 0 0" VerticalAlignment="Center"  FontFamily="PingFangSC"  FontSize="16" >
