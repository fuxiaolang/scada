                                        <GradientStop Color="#722493D3" Offset="0.45" />
                                        <GradientStop Color="#334DBAFF" Offset="1" />
                                    </LinearGradientBrush>
                                </Border.Background>
                            </Border>
                            <StackPanel Grid.Row="0" HorizontalAlignment="left" Orientation="Horizontal"  Margin="0 0 20 0"  >
                                <TextBlock  Foreground="#FFFFFF" Margin="16 0 0 0" VerticalAlignment="Center"  FontFamily="Fontquan-XinYiGuanHeiTi"  FontWeight="Bold" FontSize="20" >
                             充电仓总数：
                                </TextBlock>
                            <TextBlock x:Name="txtChargersCount"  Foreground="#00FDFA" Margin="16 0 0 0" VerticalAlignment="Center"  FontFamily="Fontquan-XinYiGuanHeiTi"  FontWeight="Bold" FontSize="20" >
                                7
                                </TextBlock>
                            </StackPanel>
                        </Grid>
                    </StackPanel>
                    <StackPanel Grid.Row="1" Grid.Column="1"  HorizontalAlignment="left" Orientation="Horizontal"  Margin="20 0 0 0"  >
                        <TextBlock  Foreground="#CEDCE6" Margin="10 0 0 0" VerticalAlignment="Center" HorizontalAlignment="Right" FontFamily="PingFangSC"  FontSize="16" >
