                    <RowDefinition  Height="40*" />
                    <RowDefinition  Height="40*" />
                    <RowDefinition  Height="40*" />
                    <RowDefinition  Height="40*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="220*"/>
                        <ColumnDefinition Width="270*"/>
                        <ColumnDefinition Width="270*"/>
                    </Grid.ColumnDefinitions>
                    <Border Grid.Row="0" Grid.ColumnSpan="3" Margin="10 0 0 0"  Height="38" Width="268"  HorizontalAlignment="Left">
