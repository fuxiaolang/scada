                                         HorizontalAlignment="Stretch" Margin="10,42,10,0" 
                                         VerticalAlignment="Top"/>

                    <StackPanel Grid.Row="1" Grid.Column="0"  HorizontalAlignment="left" Orientation="Horizontal"  Margin="20 0 0 0"  >
                    <Image  Source="/Resources/charge/充电仓概览/电池仓总数.png"  Stretch="Fill"  Width="56" Height="47" >
                    </Image>
                    <Grid>
                            <Border   CornerRadius="5"    Height="40" Width="172"  HorizontalAlignment="Left">
                                <Border.Background>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,0" Opacity="0.1">
