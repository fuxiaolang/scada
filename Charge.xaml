                        <Border  Background="#A9CDEF"  CornerRadius="5"  Opacity="0.12"  Height="40" Width="132"  HorizontalAlignment="Left"/>
                        <TextBlock  x:Name="txtFan2"  Foreground="white" Margin="16 0 0 0" VerticalAlignment="Center"   Width="132" FontFamily="PingFangSC"  FontSize="18" >
                               运行
                        </TextBlock>
                    </Grid>
                </StackPanel>

                <StackPanel Grid.Row="3" Grid.Column="0"  HorizontalAlignment="left" Orientation="Horizontal"  Margin="20 0 0 0"  >
                    <Image  Source="/Resources/charge/充电仓概览/封仓.png"  Stretch="Fill"  Width="56" Height="47" >
                    </Image>
                    <Grid>
                        <Border   CornerRadius="5"    Height="40" Width="172"  HorizontalAlignment="Left">
                            <Border.Background>
                                <LinearGradientBrush StartPoint="0,0" EndPoint="1,0" Opacity="0.1">
                                    <GradientStop Color="#A9CDEF" Offset="0.0" />
                                    <GradientStop Color="#722493D3" Offset="0.45" />
                                    <GradientStop Color="#334DBAFF" Offset="1" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                        <StackPanel Grid.Row="0" HorizontalAlignment="left" Orientation="Horizontal"  Margin="0 0 20 0"  >
