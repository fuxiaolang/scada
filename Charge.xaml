                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" Opacity="0.2">
                        <GradientStop Color="#2493D3" Offset="0.0" />
                        <GradientStop Color="#722493D3" Offset="0.45" />
                        <GradientStop Color="#334DBAFF" Offset="1" />
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Grid Margin="0,20,0,10"  Grid.Row="1"   Grid.Column="0"   >
                <Grid.RowDefinitions>
                    <RowDefinition  Height="62" />
                    <RowDefinition  Height="83*" />
                    <RowDefinition  Height="68*" />
                    <RowDefinition  Height="64*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="95*"/>
                    <ColumnDefinition Width="76*"/>
                    <ColumnDefinition Width="76*"/>
                    <ColumnDefinition Width="76*"/>
                    <ColumnDefinition Width="76*"/>
                </Grid.ColumnDefinitions>
                <Border Grid.Row="0" Grid.ColumnSpan="5" Margin="10 0 0 0" Height="38" Width="268"  HorizontalAlignment="Left">
                    <Border.Background>
                        <ImageBrush  ImageSource="/Resources/标题装饰1.png"  />
                    </Border.Background>
                </Border>
                <TextBlock Grid.Row="0" Grid.ColumnSpan="5"  Text="充电枪3" Margin="40 10 0 0"  FontFamily="{StaticResource xinyi}"  Foreground="White" FontSize="26"   />
                <Path Stretch="Fill" Data="M0,0 L10,0 z"   Width="1828" Grid.ColumnSpan="5"  Fill="#6198DD" Grid.Row="0"
                                       Stroke="#6198DD" Opacity="0.25" StrokeThickness="1"
                                       HorizontalAlignment="Stretch" Margin="10,42,10,0" 
                                       VerticalAlignment="Top"/>

                <StackPanel Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="5"   HorizontalAlignment="left" Orientation="Horizontal"  Margin="20 0 0 0"  >
                    <Image  Source="/Resources/charge/充电枪/充电枪.png"  Stretch="Fill"  Width="86" Height="83" >
                    </Image>
                    <Image x:Name="imgGunConn1"  Source="/Resources/charge/充电枪/充电连接-未连接.png" Margin="25 0 16 0" Stretch="Fill"  Width="32" Height="32" >
                    </Image>
                    <Image  Source="/Resources/charge/充电枪/货车侧面待充电.png"  Stretch="Fill"  Width="260" Height="82" >
                    </Image>
                </StackPanel>
                <TextBlock  x:Name="GunInfo1"  Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="5" Panel.ZIndex="1"  VerticalAlignment="Center" Text="请插入充电枪!" Margin="280 10 0 40"  FontFamily="{StaticResource xinyi}"  Foreground="#273F5E" FontSize="24"  />

                <StackPanel Grid.Row="2" Grid.Column="0"  Grid.ColumnSpan="5"  HorizontalAlignment="left"  VerticalAlignment="Bottom" Orientation="Horizontal"  Margin="20 0 20 0"  >
                    <StackPanel  HorizontalAlignment="left" Orientation="Vertical"  Margin="0 0 20 0"  >
                        <TextBlock  Foreground="#CEDCE6" Margin="10 0 0 0" VerticalAlignment="Center"  FontFamily="PingFangSC"  FontSize="16" >
                          X/3号枪：
                        </TextBlock>
                        <Grid>
                            <Border  Background="#A9CDEF"  CornerRadius="5"  Opacity="0.12"  Height="40" Width="266"  HorizontalAlignment="Left"/>
                            <TextBlock  x:Name="VIN1"  Foreground="white" Margin="16 0 0 0" VerticalAlignment="Center"   HorizontalAlignment="Left"  Width="132" FontFamily="PingFangSC"  FontSize="18" >
                                  -
                            </TextBlock>
                        </Grid>
                    </StackPanel>
                    <Button x:Name="btnGun1Start"  Cursor="Hand" Style="{StaticResource BtnState}"  Click="btnGun1Start_Click"  BorderThickness="0"   Margin="0 0 0 0"    Width="118" Height="40" HorizontalAlignment="Center" VerticalAlignment="Bottom" Command="{Binding BtnCommand}">
                        <Button.Background>
                            <ImageBrush ImageSource="/Resources/charge/充电枪/开始充电-禁用.png"  Stretch="Fill"></ImageBrush>
                        </Button.Background>
                    </Button>
                </StackPanel>

                <StackPanel Grid.Row="3" Grid.Column="0"  HorizontalAlignment="left" Orientation="Vertical"  Margin="20 20 0 0"  >
                    <TextBlock  Foreground="#CEDCE6" Margin="25 0 0 0" VerticalAlignment="Center"   HorizontalAlignment="left"  FontFamily="PingFangSC"  FontSize="16" >
                         电压
                    </TextBlock>
                    <Grid>
                        <Border  Background="#A9CDEF"  CornerRadius="5"   Opacity="0.12"  Height="40" Width="76"  HorizontalAlignment="Left"/>
                        <TextBlock  x:Name="ChargeU1" Foreground="white" Margin="4 0 0 0" VerticalAlignment="Center"   HorizontalAlignment="Left"  Width="76"   FontFamily="PingFangSC"  FontSize="18" >
                          -
                        </TextBlock>
                    </Grid>
                </StackPanel>
                <StackPanel Grid.Row="3" Grid.Column="1"  HorizontalAlignment="left" Orientation="Vertical"  Margin="0 20 0 0"  >
                    <TextBlock  Foreground="#CEDCE6" Margin="25 0 0 0" VerticalAlignment="Center"  FontFamily="PingFangSC"  FontSize="16" >
