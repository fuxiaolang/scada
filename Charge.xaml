                            <TextBlock  x:Name="txChargeFinishCount"  Foreground="#00FDFA" Margin="16 0 0 0" VerticalAlignment="Center"  FontFamily="Fontquan-XinYiGuanHeiTi"  FontWeight="Bold" FontSize="20" >
                          2
                            </TextBlock>
                        </StackPanel>
                    </Grid>
                </StackPanel>

            </Grid>
 
            
            <!--充电枪3-->
            <Border  CornerRadius="5" Grid.Row="1" Grid.Column="0"  Margin="0,20,0,0">
