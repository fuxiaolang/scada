<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="DefaultBorderBrushColor" >#CD7474</Color>
    <SolidColorBrush x:Key="DynamicResource MahApps.Brushes.ToggleSwitch.StrokeOff" Color="{StaticResource DefaultBorderBrushColor}"></SolidColorBrush>
    <SolidColorBrush x:Key="DynamicResource MahApps.Brushes.ToggleSwitch.KnobFillOn" Color="{StaticResource DefaultBorderBrushColor}"></SolidColorBrush>
    <SolidColorBrush x:Key="DynamicResource  MahApps.Brushes.ToggleSwitch.FillOn" Color="{StaticResource DefaultBorderBrushColor}"></SolidColorBrush>
    <SolidColorBrush x:Key="DynamicResource  MahApps.Brushes.ToggleSwitch.ContainerBackground" Color="{StaticResource DefaultBorderBrushColor}"></SolidColorBrush>
    

</ResourceDictionary>