        <!--手动操作-->
        <Border  CornerRadius="5" Grid.Row="2" Grid.Column="1" Margin="20,20,0,0">
            <Border.Background>
                <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" Opacity="0.2">
                    <GradientStop Color="#2493D3" Offset="0.0" />
                    <GradientStop Color="#722493D3" Offset="0.45" />
                    <GradientStop Color="#334DBAFF" Offset="1" />
                </LinearGradientBrush>
            </Border.Background>
        </Border>

        <Grid Margin="40,30,20,10"  Grid.Row="2"  Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition  Height="48*" />
                <RowDefinition  Height="271*" />
            </Grid.RowDefinitions>
            <Border>
                <Border.Background>
                    <ImageBrush  ImageSource="/Resources/手动操作区标题.png"    />
                </Border.Background>
            </Border>
            <StackPanel VerticalAlignment="Top"  HorizontalAlignment="Right" Orientation="Horizontal">
                <Button x:Name="bntAuto" Click="bntAuto_Click" Content="自动换电"   Style="{StaticResource BtnState}" Cursor="Hand" BorderThickness="0" Foreground="White"   Margin="10 0 0 0" FontSize="15"    FontFamily="{StaticResource xinyi}"  Width="95" Height="24" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/自动充电按钮选中.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                </Button>
                <Button x:Name="btnManual" Click="btnManual_Click" Content="手动换电"  Style="{StaticResource BtnState}" Cursor="Hand"  BorderThickness="0" Foreground="White"   Margin="10 0 0 0" FontSize="15"    FontFamily="{StaticResource xinyi}"  Width="95" Height="24" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动充电按钮选中.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                </Button>
                <Button x:Name="btnRepair" Click="btnRepair_Click" Content="检修模式"  Style="{StaticResource BtnState}" Cursor="Hand"  BorderThickness="0" Foreground="White"   Margin="10 0 0 0" FontSize="15"    FontFamily="{StaticResource xinyi}"  Width="95" Height="24" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动充电按钮选中.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                </Button>
            </StackPanel>


            <Grid Width="1315"  Height="291"   HorizontalAlignment="Right"  VerticalAlignment="Center" Grid.Row="1" >
                <Grid.RowDefinitions>
                    <RowDefinition  Height="71" />
                    <RowDefinition  Height="90" />
                    <RowDefinition Height="38"/>
                    <RowDefinition Height="90"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition  Width="243" />
                    <ColumnDefinition  Width="243" />
                    <ColumnDefinition  Width="243" />
                    <ColumnDefinition  Width="243" />
                    <ColumnDefinition  Width="297" />
                </Grid.ColumnDefinitions>

                <TextBlock x:Name="txtExchangeSteps" Text="手动换电步骤：电池定位中" Margin="9 19 0 0"  Grid.Row="0" Grid.ColumnSpan="5" FontFamily="{StaticResource xinyi}"  Foreground="#F6F9FE" FontSize="20"  />


                <Button x:Name="btnStart"  Click="btnStart_Click" Foreground="White"  Cursor="Hand" Style="{StaticResource ManualBtnSmall}"   BorderThickness="0"    Grid.Row="1" Grid.Column="0"  Margin="10 0 0 0" FontSize="30"    FontFamily="{StaticResource xinyi}"  Width="197.7" Height="77.9" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动操作按钮小默认.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                    <Button.Content>
                        <StackPanel Orientation="Horizontal">
                            <Image Stretch="None" Source="/Resources/换电.png" />
                            <TextBlock Text="换电/调仓"   Margin="9 0 0 0" />
                        </StackPanel>
                    </Button.Content>
                </Button>

                <Button x:Name="btnPause" Click="btnPause_Click"  Foreground="White"  Cursor="Hand"  Style="{StaticResource ManualBtnSmall}"   BorderThickness="0"   Grid.Row="1" Grid.Column="1"  Margin="10 0 0 0" FontSize="30"    FontFamily="{StaticResource xinyi}"  Width="197.7" Height="77.9" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动操作按钮小默认.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                    <Button.Content>
                        <StackPanel Orientation="Horizontal">
                            <Image Stretch="None" Source="/Resources/图标暂停.png" />
                            <TextBlock Text="暂停"   Margin="9 0 0 0" />
                        </StackPanel>
                    </Button.Content>
                </Button>

                <Button x:Name="btnCancel"  Click="btnCancel_Click" Foreground="White"   Cursor="Hand" Style="{StaticResource ManualBtnSmall}"  BorderThickness="0"    Grid.Row="1" Grid.Column="2"  Margin="10 0 0 0" FontSize="30"    FontFamily="{StaticResource xinyi}"  Width="197.7" Height="77.9" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动操作按钮小默认.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                    <Button.Content>
                        <StackPanel Orientation="Horizontal">
                            <Image Stretch="None" Source="/Resources/图标停止.png" />
                            <TextBlock Text="取消换电"   Margin="9 0 0 0" />
                        </StackPanel>
                    </Button.Content>
                </Button>

                <Button x:Name="btnFinish"   Click="btnFinish_Click"  Foreground="White"  Cursor="Hand" Style="{StaticResource ManualBtnSmall}"   BorderThickness="0"    Grid.Row="1" Grid.Column="3"  Margin="10 0 0 0" FontSize="30"    FontFamily="{StaticResource xinyi}"  Width="197.7" Height="77.9" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动操作按钮小默认.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                    <Button.Content>
                        <StackPanel Orientation="Horizontal">
                            <Image Stretch="None" Source="/Resources/换电完成.png" />
                            <TextBlock Text="换电完成"   Margin="9 0 0 0" />
                        </StackPanel>
                    </Button.Content>
                </Button>

                <Button x:Name="btnFaultReset"   Click="btnFaultReset_Click"  Foreground="White"  Cursor="Hand"  Style="{StaticResource ManualBtnBig}" BorderThickness="0"    Grid.Row="1" Grid.Column="4"  Margin="10 0 0 0" FontSize="30"    FontFamily="{StaticResource xinyi}"  Width="277" Height="77.9" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动操作按钮小默认.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                    <Button.Content>
                        <StackPanel Orientation="Horizontal">
                            <Image Stretch="None" Source="/Resources/故障复位.png" />
                            <TextBlock Text="故障复位"   Margin="9 0 0 0" />
                        </StackPanel>
                    </Button.Content>
                </Button>


                <!---two line-->
                <Button x:Name="btnStop"   Click="btnStop_Click"   Foreground="White"  Cursor="Hand" Style="{StaticResource ManualBtnSmall}"  BorderThickness="0"     Grid.Row="3" Grid.Column="0"  Margin="10 0 0 0" FontSize="30"    FontFamily="{StaticResource xinyi}"  Width="197.7" Height="77.9" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动操作按钮小默认.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                    <Button.Content>
                        <StackPanel Orientation="Horizontal">
                            <Image Stretch="None" Source="/Resources/换仓.png" />
                            <TextBlock Text="停止"   Margin="9 0 0 0" />
                        </StackPanel>
                    </Button.Content>
                </Button>

                <Button x:Name="btnContinue"   Click="btnContinue_Click"  Foreground="White"  Cursor="Hand" Style="{StaticResource ManualBtnSmall}"   BorderThickness="0"    Grid.Row="3" Grid.Column="1"  Margin="10 0 0 0" FontSize="30"    FontFamily="{StaticResource xinyi}"  Width="197.7" Height="77.9" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动操作按钮小默认.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                    <Button.Content>
                        <StackPanel Orientation="Horizontal">
                            <Image Stretch="None" Source="/Resources/图标继续.png" />
                            <TextBlock Text="继续"   Margin="9 0 0 0" />
                        </StackPanel>
                    </Button.Content>
                </Button>

                <Button x:Name="btnGoHome"   Click="btnGoHome_Click"    Foreground="White"  Cursor="Hand" Style="{StaticResource ManualBtnSmall}"   BorderThickness="0"    Grid.Row="3" Grid.Column="2"  Margin="10 0 0 0" FontSize="30"    FontFamily="{StaticResource xinyi}"  Width="197.7" Height="77.9" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动操作按钮小默认.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                    <Button.Content>
                        <StackPanel Orientation="Horizontal">
                            <Image Stretch="None" Source="/Resources/复归.png" />
                            <TextBlock Text="换电复归"   Margin="9 0 0 0" />
                        </StackPanel>
                    </Button.Content>
                </Button>

                <Button x:Name="btnSaveRecord"     Click="btnSaveRecord_Click"   Foreground="White"  Cursor="Hand" Style="{StaticResource ManualBtnSmall}" BorderThickness="0"    Grid.Row="3" Grid.Column="3"  Margin="10 0 0 0" FontSize="30"    FontFamily="{StaticResource xinyi}"  Width="197.7" Height="77.9" HorizontalAlignment="Center" VerticalAlignment="Top" Command="{Binding BtnCommand}">
                    <Button.Background>
                        <ImageBrush ImageSource="/Resources/手动操作按钮小默认.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                    <Button.Content>
                        <StackPanel Orientation="Horizontal">
                            <Image Stretch="None" Source="/Resources/保存记录.png" />
                            <TextBlock Text="保存记录"   Margin="9 0 0 0" />
                        </StackPanel>
                    </Button.Content>
                </Button>

                <Button x:Name="btnFire"   Click="btnFire_Click"   BorderThickness="0"  Cursor="Hand" Style="{StaticResource ManualBtnBig}" Foreground="White"    Grid.Row="3" Grid.Column="4"  Margin="10 0 0 0" FontSize="30"    FontFamily="{StaticResource xinyi}"  Width="277" Height="77.9" HorizontalAlignment="Center" VerticalAlignment="Top" >
                    <Button.Background>
                        <ImageBrush x:Name="ImgBtnBg3"  ImageSource="/Resources/手动操作按钮大默认.png"  Stretch="Fill"></ImageBrush>
                    </Button.Background>
                    <Button.Content>
                        <StackPanel Orientation="Horizontal">
                            <Image Stretch="None" Source="/Resources/系统复位.png" />
                            <TextBlock Text="一键消防"   Margin="9 0 0 0" />
                        </StackPanel>
                    </Button.Content>
                </Button>

            </Grid>

        </Grid>

    </Grid>

</UserControl>
