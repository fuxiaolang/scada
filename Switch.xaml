                    <TextBlock  FontFamily="{StaticResource xinyi}"  FontSize="16" Foreground="White">视角4</TextBlock>
                </ComboBoxItem>
                <ComboBoxItem>
                    <TextBlock  FontFamily="{StaticResource xinyi}"  FontSize="16" Foreground="White">视角5</TextBlock>
                </ComboBoxItem>
                <ComboBoxItem>
                    <TextBlock  FontFamily="{StaticResource xinyi}"  FontSize="16" Foreground="White">视角6</TextBlock>
                </ComboBoxItem>
                <ComboBoxItem>
                    <TextBlock  FontFamily="{StaticResource xinyi}"  FontSize="16" Foreground="White">视角7</TextBlock>
                </ComboBoxItem>
                <ComboBoxItem>
                    <TextBlock  FontFamily="{StaticResource xinyi}"  FontSize="16" Foreground="White">视角8</TextBlock>
                </ComboBoxItem>
                <ComboBox.Background>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,0" Opacity="0.2">
                        <GradientStop Color="#FFA9CDEF" Offset="0.0" />
